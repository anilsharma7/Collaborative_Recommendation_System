      "source": [
        "**Define a function to create a desired output**"
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "bSbIVKOmgsHK",
        "colab_type": "code",
        "colab": {}
      },
      "source": [
        "def create_output(model, users_to_recommend, n_rec, print_csv=True):\n",
        "    recomendation = model.recommend(users=users_to_recommend, k=n_rec)\n",
        "    df_rec = recomendation.to_dataframe()\n",
        "    df_rec['recommendedProducts'] = df_rec.groupby([user_id])[item_id] \\\n",
        "        .transform(lambda x: '|'.join(x.astype(str)))\n",
        "    df_output = df_rec[['customerId', 'recommendedProducts']].drop_duplicates() \\\n",
        "        .sort_values('customerId').set_index('customerId')\n",
        "    if print_csv:\n",
        "        df_output.to_csv('/content/output_option1_recommendation.csv')\n",
        "        print(\"An output file can be found in 'content' folder with name 'output_option1_recommendation.csv'\")\n",
        "    return df_output"
      ],
      "execution_count": 49,
      "outputs": []
    },
